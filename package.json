{
  "private": true,
  "scripts": {
    "dev": "npm run development",
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "npm run development -- --watch",
    "watch-poll": "npm run watch -- --watch-poll",
    "hot": "cross-env NODE_ENV=development HMR_PORT=8090 node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --disable-host-check --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod": "npm run production",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "branch-changed": "npm install && composer install && npm run ziggy-generate && php artisan permission:cache-reset",
    "start-queue": "php artisan queue:work --tries=3 --queue=line,stripe,emails,default",
    "start-ngrok": "ngrok http laradock-bcenew_apache2_1:443",
    "remove-local-branches": "git fetch -p && for branch in $(git branch -vv | grep ': gone]' | awk '{print $1}'); do git branch -D $branch; done",
    "ziggy-generate": "php artisan ziggy:generate \"resources/generated/ziggy.js\""
  },
  "devDependencies": {
    "axios": "^0.18",
    "cross-env": "^5.2.0",
    "fullcalendar": "^3.10.0",
    "jquery": "^3.2",
    "jquery-ui": "^1.12.1",
    "laravel-mix": "^4.0.15",
    "lodash": "^4.17.5",
    "moment": "^2.24.0",
    "popper.js": "^1.12",
    "sass": "^1.16.1",
    "sass-loader": "^7.1.0",
    "vue": "^2.6.11",
    "vue-template-compiler": "^2.6.11"
  },
  "dependencies": {
    "@fullcalendar/bootstrap": "^4.3.0",
    "@fullcalendar/core": "^4.3.0",
    "@fullcalendar/daygrid": "^4.3.0",
    "@fullcalendar/interaction": "^4.3.0",
    "@fullcalendar/timegrid": "^4.3.0",
    "@fullcalendar/vue": "^4.3.1",
    "@simonwep/pickr": "^1.4.2",
    "@stripe/stripe-js": "^1.8.0",
    "animate.css": "^3.7.0",
    "bootstrap": "^4.4.1",
    "bootstrap-vue": "^2.5.0",
    "css-loader": "^3.2.0",
    "datatables.net-dt": "^1.10.19",
    "dropzone": "^5.5.1",
    "eonasdan-bootstrap-datetimepicker": "^4.17.47",
    "font-awesome": "^4.7.0",
    "highcharts": "^8.0.0",
    "highcharts-vue": "^1.3.5",
    "moment-timezone": "^0.5.27",
    "selectize": "^0.12.6",
    "style-loader": "^1.0.0",
    "sweetalert2": "^9.7.1",
    "toastr": "^2.1.4",
    "v-select2-component": "^0.4.1",
    "vue-color": "^2.7.0"
  }
}
